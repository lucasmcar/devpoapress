<%- include('../../partials/header') %>


<div class="container">
    <form method="POST" action="/authenticate">
        <div class="form-group">
            <label for="useremail">Email address</label>
            <input type="email" name="logemail" class="form-control" aria-describedby="emailHelp" placeholder="Entre com o seu email">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
            <span id="error"></span>
        </div>
        <div class="form-group">
            <label for="userpass">Password</label>
            <input type="password" name="logpass" class="form-control" placeholder="Password">
        </div>
        <div class="form-check">
            <input type="checkbox" class="form-check-input" id="exampleCheck1">
            <label class="form-check-label" for="exampleCheck1">Check me out</label>
        </div>
        <button type="submit" id="create" class="btn btn-primary">Cadastrar</button>
  </form>
</div>